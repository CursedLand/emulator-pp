<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>emulator_pp</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AsmResolver.DotNet" Version="5.2.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Echo">
      <HintPath>emulator-pp\netstandard2.0\Echo.dll</HintPath>
    </Reference>
    <Reference Include="Echo.ControlFlow">
      <HintPath>emulator-pp\netstandard2.0\Echo.ControlFlow.dll</HintPath>
    </Reference>
    <Reference Include="Echo.DataFlow">
      <HintPath>emulator-pp\netstandard2.0\Echo.DataFlow.dll</HintPath>
    </Reference>
    <Reference Include="Echo.Platforms.AsmResolver">
      <HintPath>emulator-pp\netstandard2.0\Echo.Platforms.AsmResolver.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
